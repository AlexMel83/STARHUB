{
  "name": "starhub_back",
  "version": "1.0.0",
  "main": "./src/server.cjs",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --fix . && prettier -w .",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.cjs",
    "start": "cross-env NODE_ENV=production node ./src/server.cjs",
    "db:clean": "cd ./sql/ && knex migrate:rollback",
    "db:setup": "knex --knexfile ./sql/knexfile.cjs migrate:latest && knex --knexfile ./sql/knexfile.cjs seed:run",
    "devdb:clean": "cd ./sql/ && cross-env NODE_ENV=development knex migrate:rollback",
    "devdb:setup": "cd ./sql/ && cross-env NODE_ENV=development knex migrate:latest && cross-env NODE_ENV=development knex seed:run",
    "knex": "knex --cwd ./sql",
    "precommit": "eslint --fix . && prettier --write ."
  },
  "keywords": [],
  "author": "AlexMel83",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "geodata": "^0.0.1",
    "ipsp-js-sdk": "^2.0.11-alpha.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "liqpay": "^0.0.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-geocoder": "^4.3.0",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^6.9.7",
    "openid-client": "^5.6.5",
    "pg": "^8.11.3",
    "set-value": "^4.1.0",
    "update": "^0.4.2",
    "uuid": "^10.0.0"
  },
  "description": "Api for StarHub project"
}
